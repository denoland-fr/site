{
  "version": 2,
  "builds": [{
    "src": "*.md",
    "use": "@now/md",
    "config": {
      "title": "Deno for korea",
      "css": [
        "/static/index.css"
      ]
    }
  }, {
    "src": "updates/index.md",
    "use": "@now/md",
    "config": {
      "title": "What's new in Deno",
      "css": [
        "/static/index.css"
      ],
      "meta": [
        {"name": "twitter:card", "content": "summary"},
        {"name": "twitter:site", "content": "@denoland_kr"},
        {"name": "twitter:creator", "content": "@ragintwind"},
        {"name": "twitter:image", "content": "https://denoland.kr/static/deno.land.jpg"},
        {"property": "og:url", "content": "https://denoland.kr/updates"},
        {"property": "og:title", "content": "What's new in Deno"},
        {"property": "og:description", "content": "새로운 Deno 소식들을 알아보세요."},
        {"property": "og:image", "content": "https://denoland.kr/static/deno.land.jpg"}
      ]
    }
  }, {
    "src": "./static/*.*",
    "use": "@now/static"
  }]
}